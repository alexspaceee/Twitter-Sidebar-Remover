{
  "manifest_version": 2,
  "name": "Twitter Sidebar Remover",
  "version": "1.0",
  "permissions": ["activeTab", "webNavigation", "https://twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
